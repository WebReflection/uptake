{
  "name": "uptake",
  "version": "0.1.0",
  "description": "Run a Bun cluster with a shared \"worker\" module",
  "main": "index.js",
  "module": "index.js",
  "type": "module",
  "bin": {
    "uptake": "cli.js"
  },
  "scripts": {
    "test:bun": "PORT=3001 bun cli.js --main test/main.js --worker test/worker.js",
    "test:deno": "PORT=3002 deno --unstable-net cli.js --main test/deno.js --worker test/worker.js"
  },
  "files": [
    "index.js",
    "cli.js",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/uptake.git"
  },
  "keywords": [
    "bun",
    "deno",
    "cluster",
    "worker"
  ],
  "author": "Andrea Giammarchi (https://github.com/WebReflection)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WebReflection/uptake/issues"
  },
  "homepage": "https://github.com/WebReflection/uptake#readme"
}
